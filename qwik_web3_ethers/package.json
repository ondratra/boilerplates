{
  "name": "boilerplate-qwik-web3-ethers",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "checks": "yarn checks.lint",
    "checks.lint": "eslint \"src/**/*.ts*\"",
    "checks.lint.fix": "yarn checks.lint --fix",
    "clean": "rm -r dist",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "//prepare": "//husky install can only be run after this directory is copied from boilerplate repo to target repo",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.16.2",
    "@builder.io/qwik-city": "0.0.128",
    "@builder.io/qwik-react": "0.2.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/x-data-grid": "5.17.8",
    "@types/eslint": "8.4.10",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "latest",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@web3-react/core": "8.0.35-beta.0",
    "@web3-react/metamask": "8.0.30-beta.0",
    "@web3-react/network": "8.0.27-beta.0",
    "@web3-react/walletconnect": "8.0.37-beta.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "0.16.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "ethers": "^5.7.2",
    "husky": "^8.0.3",
    "node-fetch": "3.3.0",
    "prettier": "2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4",
    "undici": "5.14.0",
    "vite": "4.0.3",
    "vite-tsconfig-paths": "3.5.0"
  },
  "engines": {
    "node": ">=18.11.18"
  }
}
